extends ../layouts/master

block title
	|#{title}

block styles
	link(rel='stylesheet', href='/stylesheets/admin.css')

block scripts
	script(type='text/javascript',src='/javascripts/admin.js')

block content

	include ../includes/info-box

	header
		h1 #{header}

	section
		a.buttonstyle(href='/admin/export') Export to Spreadsheet
		a.match(href='#') Match
		table.style3
			tr.style3
					th.style3 Project name
					th.style3 Allotted Student name
					th.style3 Overwrite
					th.style3 Student ID
					th.style3 Primary Major
					th.style3 Secondary Major
					th.style3 Level In School
					th.style3 Grad Date
					th.style3 GPA
					th.style3 Gender
					th.style3 Ethnicity
					th.style3 Previous Experience
					th.style3 Other Employment
					th.style3 Student's Preferences
			tr.style3
			each project,i in projects
				tr
					td.style3
						p( id="projectname"+i) #{project.description} 
					td.style3
						p( id="AllottedStudentname"+i)
					td.style3 
						p#Overwrite
					td.style3 
						p#sid
					td.style3 
						p#primajor
					td.style3 
						p#secmajor
					td.style3 
						p#levelinschool
					td.style3 
						p#graddate
					td.style3 
						p#gpa
					td.style3 
						p#gender
					td.style3 
						p#ethnicity
					td.style3 
						p#previousexperience
					td.style3 
						p#otheremployment
					td.style3 
						p#preferences


